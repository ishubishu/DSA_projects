#include<bits/stdc++.h>
using namespace std;
void reverse(vector <int>& arr){
	int n=arr.size();
	for(int j=0;j<n/2;j++){
			swap(arr[j],arr[n-j-1]);
			
		}
	
	
}
int main(){
	vector<int> arr;
	int n,a;
	cin>>n;
	for(int i=0;i<n;i++){
		cin>>a;
		arr.push_back(a);
	}
	reverse(arr);
	for(int j=0;j<n;j++){
		cout<<arr[j]<<" ";
	}
	
	
	return 0;
}
